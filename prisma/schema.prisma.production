// Production Prisma schema for PostgreSQL
// Copy this to schema.prisma before deploying to Render
// Or manually change provider from "sqlite" to "postgresql"

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

