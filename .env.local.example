# Local Development Environment Variables
# Copy this file to .env.local for local development
# This file is gitignored - never commit your actual .env.local

# Database - Local PostgreSQL instance
# Make sure PostgreSQL is running locally on port 5432
# Create database: createdb ontrack_dev
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/ontrack_dev

# NextAuth Configuration
# Generate secret with: openssl rand -base64 32
NEXTAUTH_SECRET=your-local-dev-secret-here
NEXTAUTH_URL=http://localhost:3000

# API URLs
NEXT_PUBLIC_API_URL=http://localhost:3000
